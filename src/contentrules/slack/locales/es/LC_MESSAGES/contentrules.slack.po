#: contentrules/slack/actions/slack.pt:20
msgid "slack-message-builder"
msgstr "Constructor de mensajes"

#. Default: "Substitution"
#: contentrules/slack/actions/slack.pt:30
msgid "substitution-contentrules-slacksub"
msgstr "Sustitución"

#. Default: "Substitutions"
#: contentrules/slack/actions/slack.pt:24
msgid "title_contentrules_slacksub"
msgstr "Sustituciones"

